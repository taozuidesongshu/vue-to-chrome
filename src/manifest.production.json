{
    "manifest_version": 3,
    "name": "vue开发",
    "description": "chrome扩展+vue",
    "version": "0.0.1",
    "options_page": "html/music.html",
    "action": {
        "default_icon": "assets/extension-icons/logo-32.png",
        "default_popup": "html/popup.html"
    },
    "background": {
        "service_worker":"js/background.js",
        "type": "module"
    },
    "icons": {
        "128": "assets/extension-icons/logo-128.png",
        "48": "assets/extension-icons/logo-48.png"
    },
    "permissions": [
        "bookmarks",
        "cookies",
        "webRequest",
        "storage",
        "contextMenus",
        "tabs",
        "history",
        "activeTab"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.juejin.cn/*"
        ],
        "css": [
            "css/juejin.css"
        ],
        "js": [
            "js/juejin.js"
        ],
        "run_at": "document_end"
    },{
        "matches": [
            "https://pf.suntekcorps.com/*"
        ],
        "css": [
            "css/OT.css"
        ],
        "js": [
            "js/OT.js"
        ],
        "run_at": "document_end"
    }],
    "content_security_policy":{
        "extension_pages":"script-src 'self'; object-src 'self'"
    },
    "web_accessible_resources": [
        {
            "resources": ["fonts/*","css/content.css"],
            "matches": [ "*://*.juejin.cn/*"],
            "use_dynamic_url": true
        }
    ]
}
{
    "name": "vue element",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_title": "Vue Popup",
        "default_popup": "popups/index.html"
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "background": {
        "service_worker": "background.ts",
        "type": "module"
    },
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-scripts/juejin/main.ts"
            ],
            "css": [
                "content-scripts/juejin/main.css"
            ],
            "run_at": "document_start"
        }
    ],
    "options_ui": {
        "page": "./options/index.html"
    }
}

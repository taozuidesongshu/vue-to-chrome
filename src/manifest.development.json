{
    "manifest_version": 2,
    "name": "音乐播放",
    "description": "a chrome extension with vue-cli3",
    "version": "0.0.1",
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "assets/extension-icons/logo-32.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["hot-reload.js","js/background.js"]
    },
    "icons": {
        "16": "assets/extension-icons/logo-16.png",
        "128": "assets/extension-icons/logo-128.png",
        "48": "assets/extension-icons/logo-48.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "matches": [
            "*://*.juejin.cn/*"
        ],
        "css": [
            "css/content.css"
        ],
        "js": [
            "js/content.js"
        ],
        "run_at": "document_end"
    }],
    "web_accessible_resources": ["fonts/*","css/content.css"]
}